<!-- views/pages/index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

    <header>
        <% include ../partials/header %>
    </header>

    <main>
     <table class="table table-striped">
        <% for(var i=0; i<data.length; i++) {%>
        <tr>
        <td><img src="images/photos/<%=data[i].photo%>"> </td>
        <td><%=data[i].nom%></td>
        <td><%=data[i].prenom%></td>
        <td><%=data[i].entreprise%></td>

        <% } %>
     </table>
    </main>

    <footer>
        <% include ../partials/footer %>
    </footer>
    
</body>
</html>