<form id =  "uploadForm" enctype =  "multipart/form-data" action =  "/file/upload" method =  "post">
    <div class="form-group">
      <label for="exampleInputEmail1">Nom du dossier </label>
      <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Nom du dossier">
    </div>
    <div class="form-group">
      <label for="exampleInputPassword1">Créateur</label>
      <input type="text" class="form-control" id="exampleInputPassword1" placeholder="Créateur">
    </div>
    <div class="form-group">
      <label for="exampleInputFile">Icone</label>
      <input type="file" id="exampleInputFile">
    </div>

    <button type="submit" class="btn btn-default">Valider</button>
</form>