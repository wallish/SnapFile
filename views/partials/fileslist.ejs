<div style="clear:both"></div>
<table class="table table-striped files-list">
<tr>
    <td></td>
    <td>Nom</td>
    <td>Nombre de téléchargement</td>
    <td>Crée par</td>
    <td>Date d'ajout</td>
    <td>Action</td>
</tr>
    <% if(typeof data != 'undefined'){ %>
    <% for(var i=0; i<data.length; i++) {%>
    <tr>
    <td><img src="/images/filetype/<%=data[i].type%>.png" width="64px"/></td>
    <td><%=data[i].originalName%></td>
    <td><%=data[i].downloadCount%></td>
    <td><%=data[i].creator%></td>
    <td><%=data[i].dateinsert%></td>
    <td>
        <a href="/<%=folder+"/"+data[i].originalName%>" type="button" class="btn btn-primary btn-xs">Télécharger</a>
        <button type="button" id="<%=data[i].originalName%>" class="btn btn-danger btn-xs delete-file">Supprimer</button>
    </td>
    <% }} %>
</tr>
</table>